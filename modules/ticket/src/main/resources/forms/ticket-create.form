        },
        {
            "id": "category",
            "name": "分类",
            "type": "dropdown",
            "options": [
                {"id": "TECHNICAL", "name": "技术支持"},
                {"id": "BILLING", "name": "账单问题"},
                {"id": "GENERAL", "name": "一般咨询"}
